. /usr/lib/web/constant.sh
. /usr/lib/web/xmlconf.sh
. /usr/lib/web/constant.sh
. /usr/lib/web/sysinit.sh
local xml_config=`config_read /config/system`
xml_config=`config_setoption "$xml_config" workmode "1"`
config_write /config/system "$xml_config"
sysctl -w dev.wifi0.thinap=0
stop_monitor
if [ -f /jffs/config.wtp ];then
    wtpd > /dev/null 2>&1 &
else
    cp -f /etc/wlan/config.wtp /jffs
    wtpd > /dev/null 2>&1 &
fi
    cp -f /jffs/config.wtp /tmp
/usr/sbin/ap_monitor &
sys_emenu_wlan_setup
sys_emenu_macfilter_setup
