#!/bin/sh

PRO_CTL_UTIL=/usr/bin/pro_ctl_util

case $1 in
  sysled) 
		if [ "$2" == "enable" ];then
    	$PRO_CTL_UTIL -b 1
    else if [ "$2" == "disable" ];then
      $PRO_CTL_UTIL -d 1
    else if [ "$2" == "blink" ];then
      $PRO_CTL_UTIL -u 1	
    else 
    	echo "wrong input" 		
		fi
		fi
		fi
        ;;	
esac        
