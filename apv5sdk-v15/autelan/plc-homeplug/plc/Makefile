#!/usr/bin/make -f
# file: plc/Makefile

# ====================================================================
# environment definitions;
# --------------------------------------------------------------------

include ../make.def

# ====================================================================
# symbols;
# --------------------------------------------------------------------

CFLAGS+=-DAR7x00 
LDFLAGS+=
TRASH=*.o *.obj *.exe t t.* *~ *.[0-9][0-9][0-9]
FILES=Makefile *.mak *.sh *.txt *.c
TOOLS=int6k int6kboot int6keth showsta int6khost int64host int6kid int6klist int6klog int6kmdio int6kmdio2 int6kmod int6kstat int6ktest int6krate int6krule int6ktone int6kwait CMEncrypt sada coqos_add coqos_info coqos_man coqos_mod coqos_rel mdustats
TOOLS+=ampboot amphost ampID amplist amprate ampstat amptest amptool amptone ampwait
TOOLS+=plcboot plchost plcID plclist plcrate plcrule plcstat plctest plctool plctone plcwait plchostd plcget plcset plcotst plcfwd plcdevs plclog plcmdio16 plcmdio32
PAGES=plc.7
PAGES+=int6k.7 int6kboot.7 int6keth.7 int6kid.7 int6khost.7 int64host.7 int6klist.7 int6klog.7 int6kmdio.7 int6kmdio2.7 int6kmod.7 int6kstat.7 int6ktest.7 int6ktone.7 int6krate.7 int6krule.7 int6kwait.7 CMEncrypt.7 sada.7 coqos_add.7 coqos_info.7 coqos_man.7 coqos_mod.7 coqos_rel.7 mdustats.7
PAGES+=ampboot.7 amphost.7 ampID.7 amplist.7 amprate.7 ampstat.7 amptest.7 amptool.7 amptone.7 ampwait.7
PAGES+=plcboot.7 plchost.7 plcID.7 plclist.7 plcrate.7 plcrule.7 plcstat.7 plctest.7 plctool.7 plctone.7 plcwait.7 plchostd.7 plcget.7 plcset.7 plcotst.7 plcfwd.7 plcdevs.7 plclog.7 plcmdio16.7 plcmdio32.7 

# ====================================================================
# pseudo targets;
# --------------------------------------------------------------------

all compile: ${TOOLS}
compact: compile
	if [ ! -z ${STRIP} ]; then ${STRIP} ${TOOLS}; fi
	if [ -x /usr/local/bin/upx ]; then upx --best ${TOOLS}; fi
library:
	install -d ${BIN}
scripts:
manuals:
	install ${PAGES} ${MAN}
install: compile library
	install ${TOOLS} ${BIN}
uninstall:
	cd ${BIN}; rm -f int6k ${TOOLS}
	cd ${MAN}; rm -f int6k ${PAGES}
check:
	${SHELL} plc.sh
fresh: clean compile
clean:
	chmod 0644 *.c *.h *.mak
	chmod 0755 Makefile *.sh
	rm -f ${TRASH} ${TOOLS}

# ====================================================================
# programs;     
# --------------------------------------------------------------------

%: %.o
	${CC} -o ${@} ${^} ${LDFLAGS}
%.o: %.c
	${CC} -c ${<} ${CFLAGS}

# ====================================================================
# programs;     
# --------------------------------------------------------------------

